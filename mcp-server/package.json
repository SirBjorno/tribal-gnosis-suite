{
  "name": "tribal-gnosis-mcp-server",
  "version": "1.0.0",
  "description": "Model Context Protocol server for Tribal Gnosis application monitoring and management",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx --watch src/index.ts",
    "test": "npm run build && node dist/test.js",
    "clean": "rimraf dist",
    "prepare": "npm run build",
    "health": "npm run build && node cli.js health",
    "metrics": "npm run build && node cli.js metrics",
    "diagnostics": "npm run build && node cli.js diagnostics"
  },
  "keywords": ["mcp", "model-context-protocol", "monitoring", "render", "tribal-gnosis"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0",
    "axios": "^1.7.7",
    "ws": "^8.18.0",
    "node-cron": "^3.0.3",
    "winston": "^3.15.0",
    "express": "^4.19.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@types/node": "^22.7.5",
    "@types/ws": "^8.5.12",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/node-cron": "^3.0.11",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "rimraf": "^6.0.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}