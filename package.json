{
  "name": "tribal-gnosis",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "workspaces": [
    "tribal-gnosis-backend"
  ],
  "scripts": {
    "dev": "concurrently \"npm:dev:frontend\" \"npm:dev:backend\"",
    "dev:frontend": "vite",
    "dev:backend": "npm run dev -w tribal-gnosis-backend",
    "build": "npm run clean && npm run build:backend && npm run build:frontend",
    "build:frontend": "tsc --noEmit && vite build",
    "build:backend": "npm run build -w tribal-gnosis-backend",
    "clean": "rimraf dist tribal-gnosis-backend/dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "serve": "vite preview",
    "test": "vitest run",
    "prepare": "husky install"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "husky": "^8.0.0",
    "lint-staged": "^14.0.1",
    "postcss": "^8.4.38",
    "prettier": "^3.6.2",
    "rimraf": "^5.0.1",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vitest": "^0.34.4"
  }
}
