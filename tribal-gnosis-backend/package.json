{
  "name": "tribal-gnosis-backend",
  "version": "1.0.0",
  "description": "Backend server for Tribal Gnosis app",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "npm install -g typescript && tsc",
    "postbuild": "cp package.json dist/ && cd dist && npm install --production",
    "seed": "ts-node src/scripts/seedMaster.ts"
  },
  "preinstall": "npx only-allow npm",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google/genai": "^1.19.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "mongoose": "^8.18.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/mongoose": "^5.11.96",
    "@types/node": "^20.19.17",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5"
  }
}
